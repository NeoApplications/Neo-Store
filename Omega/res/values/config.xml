<resources>
    <!-- String representing the fragment class for settings activity.-->
    <string name="settings_fragment_name" translatable="false">com.saggitt.omega.settings.SettingsActivity$LauncherSettingsFragment</string>
    <string name="user_event_dispatcher_class" translatable="false">com.saggitt.omega.predictions.OmegaAppPredictor</string>
    <string name="icon_provider_class" translatable="false">com.saggitt.omega.icons.CustomIconProvider</string>
    <string name="drawable_factory_class" translatable="false">com.saggitt.omega.icons.CustomDrawableFactory</string>
    <string name="config_default_search_provider" translatable="false">com.saggitt.omega.search.providers.GoogleSearchProvider</string>
    <string name="default_live_wallpaper" translatable="false">com.breel.wallpapers/com.breel.wallpapers.dioramas.lagos.LagosWallpaperService</string>
    <string name="app_filter_class" translatable="false">com.saggitt.omega.allapps.CustomAppFilter</string>
    <!-- Name of a custom shortcut class. -->
    <string name="custom_shortcut_class" translatable="false">com.saggitt.omega.views.CustomEditShortcut</string>

    <!-- The default icon packs to use -->
    <!-- (Deprecated) The default icon pack to use; empty = system icons -->
    <string name="config_default_icon_pack" translatable="false" />
    <string-array name="config_default_icon_packs">
        <item>@string/config_default_icon_pack</item>
    </string-array>

    <!-- Whitelisted package to retrieve packagename for badge. Can be empty. -->
    <string name="shortcutinfocompat_badgepkg_whitelist" translatable="false"></string>

    <bool name="config_default_enable_blur">true</bool>
    <!-- Tag id used for view scrim -->
    <item name="view_scrim" type="id" />

    <!-- Out of 100, the percent to shrink the workspace during options mode. -->
    <integer name="config_workspaceOptionsShrinkPercentage">70</integer>

    <string name="action_do_nothing_class" translatable="false">com.saggitt.omega.gestures.BlankGestureHandler</string>
    <string name="action_open_notifications_class" translatable="false">com.saggitt.omega.gestures.handlers.NotificationsOpenGestureHandler</string>
    <string name="action_open_drawer_class" translatable="false">com.saggitt.omega.gestures.handlers.OpenDrawerGestureHandler</string>
    <string name="action_open_overview_class" translatable="false">com.saggitt.omega.gestures.handlers.OpenOverviewGestureHandler</string>

</resources>